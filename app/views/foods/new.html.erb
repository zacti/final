<%= form_for @food do |f| %>

<div class="food_field">
<%= f.label :name %>
<%= f.text_field :name %>
</div>

<div class="food_field">
<%= f.label :location %>
<%= f.text_field :location %>
</div>

<div class="furniture_field">
<%= f.label :price %>
<%= f.text_field :location %></div>

<div class="furniture_field">
<%= f.file_field :image %>
<%= f.submit 'Upload' %>
</div>

<% end %>

